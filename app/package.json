    "react-native-masked-text": "1.7.2",
    "react-native-modal": "6.5.0",
    "react-native-vector-icons": "5.0.0",
    "react-navigation": "1.0.0-beta.26",
    "react-redux": "5.0.6",
    "redux": "3.7.2",
    "redux-thunk": "2.2.0"
